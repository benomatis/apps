{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "test:ci",
          "lint"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "test:ci": {
      "dependsOn": [
        "^test:ci"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "deploy": {
      "dependsOn": [
        "^deploy"
      ]
    },
    "deploy:test": {
      "dependsOn": [
        "^deploy:test"
      ]
    }
  }
}
